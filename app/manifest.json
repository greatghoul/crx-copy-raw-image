{
  "manifest_version": 3,

  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "1.0.0",

  "default_locale": "en",

  "icons": {
    "48": "images/icon.png",
    "128": "images/icon.png"
  },

  "background": {
    "service_worker": "sw.js"
  },

  "permissions": [
    "activeTab",
		"contextMenus",
		"scripting",
		"notifications"
  ],

	"host_permissions": [
		"<all_urls>"
	]
}
